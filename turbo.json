{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "start:dev": {
      "persistent": true,
      "cache": false
    },
    "start:prod": {
      "persistent": true,
      "cache": true
    }
  }
}
